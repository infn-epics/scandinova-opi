  <widget type="group" version="3.0.0">
    <name>Control Interface</name>
    <x>10</x>
    <y>485</y>
    <width>1290</width>
    <height>280</height>
    <style>3</style>
    <font>
      <font family="Liberation Sans" style="BOLD" size="14.0">
      </font>
    </font>
    <foreground_color>
      <color red="0" green="0" blue="128">
      </color>
    </foreground_color>
    <line_color>
      <color red="0" green="0" blue="128">
      </color>
    </line_color>
    <widget type="label" version="2.0.0">
      <name>ControlTitle</name>
      <text>Control Interface</text>
      <width>1160</width>
      <height>30</height>
      <font>
        <font family="Liberation Sans" style="BOLD" size="16.0">
        </font>
      </font>
      <horizontal_alignment>1</horizontal_alignment>
      <rules>
        <rule name="color" prop_id="background_color" out_exp="false">
          <exp bool_exp="pvInt0==0">
            <value>
              <color name="OK" red="0" green="255" blue="0">
              </color>
            </value>
          </exp>
          <exp bool_exp="pvInt0==1">
            <value>
              <color name="Off" red="60" green="100" blue="60">
              </color>
            </value>
          </exp>
          <pv_name>$(P):$(R):Thy:Status:Ready</pv_name>
        </rule>
      </rules>
    </widget>
